page {
  background-color: #fff;
}

.icon-remark {
  color: #888;
  margin: 0;
  font-size: 28rpx;
}

.top-main {
  padding: 50rpx 90rpx;
}

.icon-box-title {
  margin: 60rpx 0 20rpx 0;
  display: flex;
  align-items: center;
}

.icon-box-desc {
  margin: 30rpx 0 70rpx 0;
  display: flex;
  align-items: center;
}

.icon-box {
  padding: 60rpx 0;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #e5e5e5;
}

.icon-box__ctn {
  flex-shrink: 100;
}

.icon-box__title {
  font-size: 20px;
}

.icon-box__desc {
  margin-top: 6px;
  font-size: 12px;
  color: #888;
}

.icon_sp_area {
  margin-top: 10px;
  text-align: left;
}

.create-icon {
  width: 50px;
  height: 50px;
  margin-right: 10px;
  border-radius: 50px;
}

.btn {
  background-color: #1aad19;
  color: #fff;
  height: 90rpx;
  line-height: 90rpx;
}

.icon-box2 {
  padding: 60rpx 0;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #e5e5e5;
  justify-content: center;
}

.icon-box__ctn2 {
  flex-shrink: 100;
  text-align: center;
}

.icon-box__title2 {
  font-size: 40rpx;
  margin: 10rpx 0;
}

.create-icon2 {
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
}

.icon-title {
  font-size: 30rpx;
  line-height: 42rpx;
}
